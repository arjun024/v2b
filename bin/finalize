#!/bin/bash
set -euo pipefail

echo "Finalize Phase"

BUILD_DIR=$1
CACHE_DIR=$2
DEPS_DIR=$3
DEPS_IDX=$4
PROFILE_DIR=${5:-}

export BUILDPACK_DIR=`dirname $(readlink -f ${BASH_SOURCE%/*})`

echo "user is $(whoami)"
echo "BUILD_DIR is $BUILD_DIR"
echo "DEPS_DIR is $DEPS_DIR"
echo "BUILDPACK_DIR is $BUILDPACK_DIR"
echo "****"
